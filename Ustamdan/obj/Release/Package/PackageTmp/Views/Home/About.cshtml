@{
    ViewBag.Title = "About";
}
<style>
    .section {
        background-color: white;
    }
</style>

<div class="container clearfix">

    <div class="col_three_fifth topmargin-sm bottommargin">
        <img data-animate="fadeInLeftBig" src="~/Content/img/hikayemiz.jpg" alt="Hikayemiz">
    </div>

    <div class="col_two_fifth topmargin-sm bottommargin-lg col_last">

        <div class="heading-block">
            <h2>@Resource.AboutStoryTitle</h2>
        </div>
        <p>
            @Resource.AboutStoryP1
        </p>
        <p>
            @Resource.AboutStoryP2
        </p>
        <p>
            @Resource.AboutStoryP3
        </p>
        <p>
            <i>
                @Resource.AboutStoryP4
            </i>
        </p>
    </div>
    <div class="clear"></div>
    <div class="section topmargin-lg">
        <div class="heading-block center">
            <h2>@Resource.IndexServicesTitle</h2>
        </div>

        <div class="clear bottommargin-sm"></div>

        <div class="container clearfix">
            <div class="col_one_fourth nobottommargin">
                <div class="feature-box fbox-center fbox-light fbox-effect nobottomborder">
                    <div class="fbox-icon">
                        <a href="#"><i class="i-alt noborder icon-folder-open"></i></a>
                    </div>
                    <h3>@Resource.IndexService1Title</h3>
                    <i style="font-size:13px;">@Resource.IndexService1Text</i>
                </div>
            </div>

            <div class="col_one_fourth nobottommargin">
                <div class="feature-box fbox-center fbox-light fbox-effect nobottomborder">
                    <div class="fbox-icon">
                        <a href="#"><i class="i-alt noborder icon-gift"></i></a>
                    </div>
                    <h3>@Resource.IndexService2Title</h3>
                    <i style="font-size:13px;">@Resource.IndexService2Text</i>
                </div>
            </div>

            <div class="col_one_fourth nobottommargin">
                <div class="feature-box fbox-center fbox-light fbox-effect nobottomborder">
                    <div class="fbox-icon">
                        <a href="#"><i class="i-alt noborder icon-thumbs-up"></i></a>
                    </div>
                    <h3>@Resource.IndexService3Title</h3>
                    <i style="font-size:13px;">@Resource.IndexService3Text</i>
                </div>
            </div>

            <div class="col_one_fourth nobottommargin col_last">
                <div class="feature-box fbox-center fbox-light fbox-effect nobottomborder">
                    <div class="fbox-icon">
                        <a href="#"><i class="i-alt noborder icon-picture"></i></a>
                    </div>
                    <h3>@Resource.IndexService4Title</h3>
                    <i style="font-size:13px;">@Resource.IndexService4Text</i>
                </div>
            </div>

            <div class="clear"></div>
            <div class="clear"></div>
        </div>
    </div>
    <div class="clear"></div>

    <div class="heading-block center">
        <h3>@Resource.AboutTeamTitle</h3>
    </div>

    <div class="row">
        <div class="col-md-3 col-sm-6 bottommargin">
            <div class="team">
                <div class="team-image">
                    <img src="~/Content/img/ekip/gokce.gif" alt="Gökçe Gülcüler" />
                </div>
                <div class="team-desc">
                    <div class="team-title"><h4>GÖKÇE GÜLCÜLER</h4><span>@Resource.TeamPosition1</span></div>
                </div>
            </div>
        </div>

        <div class="col-md-3 col-sm-6 bottommargin">
            <div class="team">
                <div class="team-image">
                    <img src="~/Content/img/ekip/berkin.png" alt="BERKİN ŞAFAK ŞENER" />
                </div>
                <div class="team-desc">
                    <div class="team-title"><h4>BERKİN ŞAFAK ŞENER</h4><span>@Resource.TeamPosition2</span></div>
                </div>
            </div>
        </div>

        <div class="col-md-3 col-sm-6 bottommargin">
            <div class="team">
                <div class="team-image">
                    <img src="~/Content/img/ekip/pelin.gif" alt="PELİN SÜRMELİ" />
                </div>
                <div class="team-desc">
                    <div class="team-title"><h4>PELİN SÜRMELİ</h4><span>@Resource.TeamPosition3</span></div>
                </div>
            </div>
        </div>

        <div class="col-md-3 col-sm-6 bottommargin">
            <div class="team">
                <div class="team-image">
                    <img src="~/Content/img/ekip/baris_dikici.png" alt="BARIŞ DİKİCİ" />
                </div>
                <div class="team-desc">
                    <div class="team-title"><h4>BARIŞ DİKİCİ</h4><span>@Resource.TeamPosition4</span></div>
                </div>
            </div>
        </div>
        <div class="col-md-3 col-sm-6 bottommargin">
            <div class="team">
                <div class="team-image">
                    <img src="~/Content/img/ekip/rey-1.gif" alt="REYHANE YAĞMUR" />
                </div>
                <div class="team-desc">
                    <div class="team-title"><h4>REYHANE YAĞMUR</h4><span>@Resource.TeamPosition5</span></div>
                </div>
            </div>
        </div>

        <div class="col-md-3 col-sm-6 bottommargin">
            <div class="team">
                <div class="team-image">
                    <img src="~/Content/img/ekip/sinem.gif" alt="SİNEM AY" />
                </div>
                <div class="team-desc">
                    <div class="team-title"><h4>SİNEM AY</h4><span>@Resource.TeamPosition6</span></div>
                </div>
            </div>
        </div>

        <div class="col-md-3 col-sm-6 bottommargin">
            <div class="team">
                <div class="team-image">
                    <img src="~/Content/img/ekip/burcu.gif" alt="BURCU ZENGİN" />
                </div>
                <div class="team-desc">
                    <div class="team-title"><h4>BURCU ZENGİN</h4><span>@Resource.TeamPosition6</span></div>
                </div>
            </div>
        </div>

        <div class="col-md-3 col-sm-6 bottommargin">
            <div class="team">
                <div class="team-image">
                    <img src="~/Content/img/ekip/ust1.png" alt="OGÜN AYGÜN" />
                </div>
                <div class="team-desc">
                    <div class="team-title"><h4>OGÜN AYGÜN</h4><span>@Resource.TeamPosition7</span></div>
                </div>
            </div>
        </div>

        <div class="col-md-3 col-sm-6 bottommargin">
            <div class="team">
                <div class="team-image">
                    <img src="~/Content/img/ekip/ust2.png" alt="DENİZ FIRAT" />
                </div>
                <div class="team-desc">
                    <div class="team-title"><h4>DENİZ FIRAT</h4><span>@Resource.TeamPosition8</span></div>
                </div>
            </div>
        </div>
        <div class="col-md-3 col-sm-6 bottommargin">
            <div class="team">
                <div class="team-image">
                    <img src="~/Content/img/ekip/halim.jpg" alt="HALİM BEYDEN" />
                </div>
                <div class="team-desc">
                    <div class="team-title"><h4>HALİM BEYDEN</h4><span>@Resource.TeamPosition9</span></div>
                </div>
            </div>
        </div>
    </div>
    <div class="clear"></div>
    <div class="section nobottommargin notopmargin ">
        <div class="container clearfix">
            <div class="heading-block center">
                <h3>@Resource.IndexPartnersTitle</h3>
            </div>

            <div id="oc-clients" class="section nobgcolor notopmargin owl-carousel owl-carousel-full image-carousel footer-stick carousel-widget dark" data-margin="80" data-loop="false" data-nav="false" data-autoplay="5000" data-pagi="true" data-items-xxs="3" data-items-xs="4" data-items-sm="4" data-items-md="4" data-items-lg="4">
                <div class="oc-item"><a href="#"><img src="~/Content/img/destekciler/gir.gif"></a></div>
                <div class="oc-item"><a href="#"><img src="~/Content/img/destekciler/things.gif"></a></div>
                <div class="oc-item"><a href="#"><img src="~/Content/img/destekciler/trpe.gif"></a></div>
                <div class="oc-item"><a href="#"><img src="~/Content/img/destekciler/ay.jpg"></a></div>
                <div class="oc-item"><a href="#"><img src="~/Content/img/destekciler/bilgi.gif"></a></div>
                <div class="oc-item"><a href="#"><img src="~/Content/img/destekciler/cek.gif"></a></div>
                <div class="oc-item"><a href="#"><img src="~/Content/img/destekciler/end.gif"></a></div>
                <div class="oc-item"><a href="#"><img src="~/Content/img/destekciler/bilgi.jpg"></a></div>
                <div class="oc-item"><a href="#"><img src="~/Content/img/destekciler/slyvian.jpg"></a></div>
                <div class="oc-item"><a href="#"><img src="~/Content/img/destekciler/tbt.jpg"></a></div>
            </div>
        </div>
    </div>

</div>